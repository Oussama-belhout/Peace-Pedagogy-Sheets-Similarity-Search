     ============================================================================ -->

<owl:Ontology rdf:about="http://www.semanticweb.org/peace-pedagogy/ontology"/>

<!-- ============================================================================
     OBJECT PROPERTIES (Relationships between concepts)
     ============================================================================ -->

<owl:ObjectProperty rdf:about="#hasAxis">
  <rdfs:label>has Peace Axis</rdfs:label>
  <rdfs:comment>Links a lesson to its peace axes</rdfs:comment>
  <rdfs:domain rdf:resource="#Lesson"/>
  <rdfs:range rdf:resource="#PeaceAxis"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usesTool">
  <rdfs:label>uses Tool</rdfs:label>
  <rdfs:comment>Links a lesson to pedagogical tools</rdfs:comment>
