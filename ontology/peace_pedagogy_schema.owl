  <rdfs:range rdf:resource="#Tool"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#employsStrategy">
  <rdfs:label>employs Strategy</rdfs:label>
  <rdfs:comment>Links a lesson to teaching strategies</rdfs:comment>
